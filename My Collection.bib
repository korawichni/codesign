keywords = {efficiency power loss,lcl-filter,llcl-filter,modulation,single-phase},
number = {1},
pages = {63--79},
title = {{Efficiency comparison between the LLCL and LCL-filters based single-phase grid-tied inverters}},
volume = {63},
year = {2014}
}
@article{Yang2018,
abstract = {IEEE In order to prevent the power switching devices (e.g., the Insulated-Gate-Bipolar-Transistor, IGBT) from shoot-through in voltage source converters during a switching period, the dead time is added either in the hardware driver circuits of the IGBTs or implemented in the software Pulse-Width Modulation (PWM) scheme. Both solutions will contribute to a degradation of the injected current power quality. As a consequence, the harmonics induced by the dead time (referred to as "dead time harmonics" hereafter) have to be compensated in order to achieve a satisfactory current as required by the standards. In this paper, the emission mechanism of dead time harmonics in single-phase PWM inverters is presented considering the modulation schemes in details. More importantly, a repetitive controller has been adopted to eliminate the dead time effect in single-phase grid-connected PWM converters. The repetitive controller has been plugged into a proportional resonant based fundamental current controller so as to mitigate the dead time harmonics and also maintain the control of the fundamental grid current in terms of dynamics. Simulations and experiments are provided, which confirm that the repetitive controller can effectively compensate the dead time harmonics and other low-order distortions, and also it is a simple method without hardware modifications.},
author = {Yang, Yongheng and Zhou, Keliang and Wang, Huai and Blaabjerg, Frede},
doi = {10.1109/TIA.2018.2825941},
file = {:D\:/virtual_circuit/mendeley/08337004.pdf:pdf},
issn = {00939994},
journal = {IEEE Trans. Ind. Appl.},
keywords = {Harmonics,PWM converters,dead time,insulated-gatebipolar-transistor (IGBT),modulation,repetitive control,resonant control},
number = {2},
pages = {5343--5354},
publisher = {IEEE},
title = {{Analysis and mitigation of dead time harmonics in the single-phase full-bridge PWM converters with repetitive controllers}},
volume = {54},
year = {2018}
}
