issn = {02780046},
journal = {IEEE Trans. Ind. Electron.},
keywords = {Current control,power conditioning,pulsewidth-modulated (PWM) power converters,resonant controllers},
number = {11},
pages = {5231--5250},
title = {{Analysis and design of resonant current controllers for voltage-source converters by means of nyquist diagrams and sensitivity function}},
volume = {58},
year = {2011}
}
