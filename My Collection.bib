keywords = {Current control,LCL filter,extended Kalman filter,sliding-mode control,voltage sensorless},
number = {7},
pages = {5637--5650},
publisher = {IEEE},
title = {{Model-based active damping control for three-phase voltage source inverters with LCL filter}},
volume = {32},
year = {2017}
}
